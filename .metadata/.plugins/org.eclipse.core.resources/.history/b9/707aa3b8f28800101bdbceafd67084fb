#include <stdint.h>
#include <stdio.h>
#include "stm32f411xE.h"
#include "uart.h"
#include "adc.h"
#include "systick.h"
#include "tim.h"

#define GPIOAEN		(1U<<0)
#define	GPIOA_5		(1U<<5)
#define LED			GPIOA_5




/* Physical jumper wire from PA5(D12) to PA6(D13) is required*/

int timestamp = 0;

int main(void)
{
	tim2_pa5_output_compare();
	tim3_pa6_input_capture();


	while(1)
	{
		while(!(TIM3->SR & SR_CC1IF)){} // wait for the rising edge
		timestamp = TIM3->CCR1;
	}

}





